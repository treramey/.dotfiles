	hunk-header-style = file line-number syntax
	line-numbers-left-style = "#6e6a86"
	line-numbers-minus-style = bold "#eb6f92"
	line-numbers-plus-style = bold "#3e8fb0"
	line-numbers-right-style = "#6e6a86"
	line-numbers-zero-style = "#6e6a86"
	minus-emph-style = bold syntax "#ea9a97"
	minus-style = syntax "#393552"
	plus-emph-style = bold syntax "#9ccfd8"
	plus-style = syntax "#2a273f"
